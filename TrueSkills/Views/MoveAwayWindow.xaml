<Window x:Class="TrueSkills.Views.MoveAwayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrueSkills.Views"
        mc:Ignorable="d"
        Title="{DynamicResource ma_Title}" MouseDown="Window_MouseDown" Height="275" Width="650" WindowStartupLocation="CenterScreen"  ResizeMode="NoResize" WindowStyle="None" FontFamily="Roboto">
    <Grid Background="{StaticResource BackColor}">
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <local:DefaultHeaderUC VisibilityLogout="Collapsed" VisibilityStudent="Collapsed"></local:DefaultHeaderUC>
        <Button Style="{StaticResource pushButton}" Grid.Row="1" Click="Button_Click" HorizontalAlignment="Center" VerticalAlignment="Center" Content="{DynamicResource ma_Return}"></Button>
    </Grid>
</Window>
